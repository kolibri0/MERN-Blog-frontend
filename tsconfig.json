{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "preserve",
    // "jsx": "react-jsx",
    "incremental": true
  },
  "include": [
    "src",
    "src/@types",
    "./types.d.ts",
    "components/axios.js", 
    "components/axios.js", 
    "Interface", 
    ],
  "exclude": [
    "node_modules",
    "./types.d.ts",
  ]
}
